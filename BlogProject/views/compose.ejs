<%- include ("partials/header") -%>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input class="form-control" type="text" name="postTitle" autocomplete="off">
    <label>Post</label>
    <textarea class="form-control" name="postBody" rows="5" cols="40"></textarea>
  </div>
  <button class="bdn btn-primary" type="submit" name="list"> Publish</button>
</form>

<%- include ("partials/footer") -%>
